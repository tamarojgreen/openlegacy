<?xml version='1.0' encoding='UTF-8'?>

<xs:schema
        xmlns:tns="http://namespace1"
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        version="1.0"
        targetNamespace="http://namespace1">

    <xs:element name="uploadXml" type="tns:uploadXml"/>
    <xs:element name="uploadXmlResponse" type="tns:uploadXmlResponse"/>

    <xs:complexType name="uploadXml">
        <xs:sequence>
            <xs:element name="user" type="xs:string" minOccurs="0"/>
            <xs:element name="password" type="xs:string" minOccurs="0"/>
            <xs:element name="appName" type="xs:string" minOccurs="0"/>
            <xs:element name="data" type="xs:string" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="uploadXmlResponse">
        <xs:sequence>
            <xs:element name="return" type="xs:string" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

</xs:schema>