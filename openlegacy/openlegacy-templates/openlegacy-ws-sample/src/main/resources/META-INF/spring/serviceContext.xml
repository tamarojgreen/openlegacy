<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context" xmlns:jaxws="http://cxf.apache.org/jaxws"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:security="http://www.springframework.org/schema/security"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd
		http://www.springframework.org/schema/security http://www.springframework.org/schema/security/spring-security-3.1.xsd
		http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">

	<import resource="classpath:META-INF/cxf/cxf.xml" />
	<import resource="classpath:META-INF/cxf/cxf-servlet.xml" />

<!-- auto generated service ItemDetails start-->
	<bean id="itemDetailsPool"
	class="org.openlegacy.terminal.support.SimpleTerminalSessionPoolFactory" p:maxConnections="2" 
	p:keepAliveAction="com.openlegacy.ws.openlegacy.services.ItemDetailsServiceImpl.KeepAliveAction" p:keepAliveInterval="600000" 
	p:initAction="com.openlegacy.ws.openlegacy.services.ItemDetailsServiceImpl.InitAction" p:cleanupAction="com.openlegacy.ws.openlegacy.services.ItemDetailsServiceImpl.CleanupAction"/>
	<bean id="itemDetailsImpl" class="com.openlegacy.ws.openlegacy.services.ItemDetailsServiceImpl" />

	<jaxws:endpoint id="itemDetails" implementor="#itemDetailsImpl" address="/itemDetails" />

	<jaxws:client id="itemDetailsClient" serviceClass="com.openlegacy.ws.openlegacy.services.ItemDetailsService"
		address="http://localhost:8080/openlegacy-ws-sample/services/itemDetails" /><!-- auto generated service ItemDetails end -->
<!-- auto generated service WarehouseDetails start-->
	<bean id="warehouseDetailsPool"
	class="org.openlegacy.terminal.support.SimpleTerminalSessionPoolFactory" p:maxConnections="2" 
	p:keepAliveAction="com.openlegacy.ws.openlegacy.services.WarehouseDetailsServiceImpl.KeepAliveAction" p:keepAliveInterval="600000" 
	p:initAction="com.openlegacy.ws.openlegacy.services.WarehouseDetailsServiceImpl.InitAction" p:cleanupAction="com.openlegacy.ws.openlegacy.services.WarehouseDetailsServiceImpl.CleanupAction"/>
	<bean id="warehouseDetailsImpl" class="com.openlegacy.ws.openlegacy.services.WarehouseDetailsServiceImpl" />

	<jaxws:endpoint id="warehouseDetails" implementor="#warehouseDetailsImpl" address="/warehouseDetails" />

	<jaxws:client id="warehouseDetailsClient" serviceClass="com.openlegacy.ws.openlegacy.services.WarehouseDetailsService"
		address="http://localhost:8080/openlegacy-ws-sample/services/warehouseDetails" /><!-- auto generated service WarehouseDetails end -->
<!-- auto generated service Items start-->
	<bean id="itemsPool"
	class="org.openlegacy.terminal.support.SimpleTerminalSessionPoolFactory" p:maxConnections="2" 
	p:keepAliveAction="com.openlegacy.ws.openlegacy.services.ItemsServiceImpl.KeepAliveAction" p:keepAliveInterval="600000" 
	p:initAction="com.openlegacy.ws.openlegacy.services.ItemsServiceImpl.InitAction" p:cleanupAction="com.openlegacy.ws.openlegacy.services.ItemsServiceImpl.CleanupAction"/>
	<bean id="itemsImpl" class="com.openlegacy.ws.openlegacy.services.ItemsServiceImpl" />

	<jaxws:endpoint id="items" implementor="#itemsImpl" address="/items" />

	<jaxws:client id="itemsClient" serviceClass="com.openlegacy.ws.openlegacy.services.ItemsService"
		address="http://localhost:8080/openlegacy-ws-sample/services/items" /><!-- auto generated service Items end -->
