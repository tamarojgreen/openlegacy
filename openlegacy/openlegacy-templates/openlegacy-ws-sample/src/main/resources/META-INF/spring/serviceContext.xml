<!-- auto generated service GetWarehouseDetails start-->
	<bean id="getWarehouseDetailsPool"
	class="org.openlegacy.terminal.support.SimpleTerminalSessionPoolFactory" p:maxConnections="2" 
	p:keepAliveAction="com.openlegacy.ws.openlegacy.services.GetWarehouseDetailsServiceImpl.KeepAliveAction" p:keepAliveInterval="600000" 
	p:initAction="com.openlegacy.ws.openlegacy.services.GetWarehouseDetailsServiceImpl.InitAction" p:cleanupAction="com.openlegacy.ws.openlegacy.services.GetWarehouseDetailsServiceImpl.CleanupAction"/>
	<bean id="getWarehouseDetailsImpl" class="com.openlegacy.ws.openlegacy.services.GetWarehouseDetailsServiceImpl" />

	<jaxws:endpoint id="getWarehouseDetails" implementor="#getWarehouseDetailsImpl" address="/getWarehouseDetails" />

	<jaxws:client id="getWarehouseDetailsClient" serviceClass="com.openlegacy.ws.openlegacy.services.GetWarehouseDetailsService"
		address="http://localhost:8080/openlegacy-ws-sample/services/getWarehouseDetails" /><!-- auto generated service GetWarehouseDetails end -->
